    {
      "parameters": {
        "jsCode": "const items = $input.all();\n\n// Get the notification log to check for duplicates\nconst notificationLog = $('Check Notification Log').all();\n\n// Build a set of already-notified combinations (email + assessment)\nconst notifiedSet = new Set();\nnotificationLog.forEach(log => {\n  if (log.json && log.json['Trainee Email'] && log.json['Assessment Name']) {\n    const key = `${log.json['Trainee Email']}_${log.json['Assessment Name']}`;\n    notifiedSet.add(key);\n  }\n});\n\nconsole.log(`Found ${notifiedSet.size} already notified assessments`);\n\n// Coach name to email mapping\nfunction getCoachEmail(coachName) {\n  const coachMapping = {\n    'Ain': 'ain.nizar@storehub.com',\n    'Andrea': 'andrea.kaur@storehub.com',\n    'Victor': 'victor.teng@storehub.com',\n    'William': 'william.yew@storehub.com',\n    'Izzuddin': 'izzuddin.azizol@storehub.com',\n    'Iwin': 'iwin.tay@storehub.com',\n    'Javelyn': 'javelyn.hee@storehub.com',\n    'Nic': 'nicholas.eng@storehub.com',\n    'Joshua': 'joshua.guevarra@storehub.com'\n  };\n  return coachMapping[coachName] || 'andrea.kaur@storehub.com';\n}\n\n// Assessment name detection\nfunction getAssessmentName(item) {\n  const sheetContext = JSON.stringify(item).toLowerCase();\n  \n  // BC ASSESSMENTS\n  if (sheetContext.includes('[bc] pitching') || sheetContext.includes('bc pitching')) {\n    return 'BC Pitching Assessment';\n  }\n  if (sheetContext.includes('[bc] spin') || sheetContext.includes('bc spin')) {\n    return 'BC SPIN Assessment';\n  }\n  if (sheetContext.includes('[bc] closing') || sheetContext.includes('bc closing')) {\n    return 'BC Closing Skills Assessment';\n  }\n  if (sheetContext.includes('[bc] full pitching') || sheetContext.includes('bc full pitching')) {\n    return 'BC Full Pitching Assessment';\n  }\n  if (sheetContext.includes('[bc] mock test') || sheetContext.includes('bc mock test')) {\n    return 'BC Mock Test';\n  }\n  \n  // LEGACY ASSESSMENTS\n  if (sheetContext.includes('hardware')) return 'Hardware Assessment';\n  if (sheetContext.includes('care mock') || sheetContext.includes('mock test attempt')) return 'Care Mock Test';\n  if (sheetContext.includes('training assessment') && sheetContext.includes('f&b')) return 'Training Assessment (F&B)';\n  if (sheetContext.includes('training assessment') && sheetContext.includes('retail')) return 'Training Assessment (Retail)';\n  if (sheetContext.includes('welcome call')) return 'Welcome Call Assessment';\n  if (sheetContext.includes('go live')) return 'Go Live Assessment';\n  if (sheetContext.includes('training mock') && sheetContext.includes('f&b')) return 'Training Mock Test (F&B)';\n  if (sheetContext.includes('training mock') && sheetContext.includes('retail')) return 'Training Mock Test (Retail)';\n  if (sheetContext.includes('roleplay')) return 'CSM Roleplay Assessment';\n  \n  return 'Assessment';\n}\n\n// Process score\nfunction processScore(scoreValue) {\n  if (!scoreValue || scoreValue === '' || scoreValue === 0) {\n    return null;\n  }\n  \n  if (typeof scoreValue === 'string' && scoreValue.includes('%')) {\n    const percentage = parseFloat(scoreValue.replace('%', ''));\n    return {\n      percentage: `${percentage}%`,\n      passStatus: percentage >= 80 ? 'PASS' : 'FAIL',\n      statusEmoji: percentage >= 80 ? '‚úÖ' : '‚ùå'\n    };\n  }\n  \n  if (scoreValue === 'PASS' || scoreValue === 'FAIL') {\n    return {\n      percentage: scoreValue === 'PASS' ? 'PASS' : 'FAIL',\n      passStatus: scoreValue,\n      statusEmoji: scoreValue === 'PASS' ? '‚úÖ' : '‚ùå'\n    };\n  }\n  \n  if (typeof scoreValue === 'number' && scoreValue <= 1) {\n    const percentage = Math.round(scoreValue * 100);\n    return {\n      percentage: `${percentage}%`,\n      passStatus: percentage >= 80 ? 'PASS' : 'FAIL',\n      statusEmoji: percentage >= 80 ? '‚úÖ' : '‚ùå'\n    };\n  }\n  \n  return null;\n}\n\n// Process items and filter duplicates\nreturn items.map(item => {\n  const scoreResult = processScore(item.json.Score);\n  \n  if (!scoreResult) {\n    return null;\n  }\n  \n  const assessmentName = getAssessmentName(item.json);\n  const traineeEmail = item.json['New Hire Email'] || 'Unknown';\n  \n  // Check if already notified\n  const uniqueKey = `${traineeEmail}_${assessmentName}`;\n  \n  if (notifiedSet.has(uniqueKey)) {\n    console.log(`‚úã Skipping duplicate: ${uniqueKey}`);\n    return null;\n  }\n  \n  console.log(`‚úÖ Processing new notification: ${uniqueKey}`);\n  \n  return {\n    json: {\n      notification_id: uniqueKey,\n      trainee_name: item.json['New Hire Full Name'] || 'Unknown',\n      trainee_email: traineeEmail,\n      coach_name: item.json['Coach Name'] || 'Unknown',\n      coach_email: getCoachEmail(item.json['Coach Name']),\n      role: item.json['Department/Role'] || 'Unknown',\n      assessment_name: assessmentName,\n      percentage_score: scoreResult.percentage,\n      pass_status: scoreResult.passStatus,\n      status_emoji: scoreResult.statusEmoji,\n      timestamp: item.json.Timestamp,\n      submission_date: new Date().toLocaleDateString(),\n      notified_date: new Date().toISOString()\n    }\n  };\n}).filter(item => item !== null);"
      },
