    {
      "parameters": {
        "jsCode": "const assessmentData = $('Code').first().json; // The current assessment completion data\nconst allTrainees = $input.all(); // All trainees from master list\n\nconst traineeName = assessmentData.trainee_name;\nconst assessmentName = assessmentData.assessment_name;\n\n// Try multiple matching strategies\nlet traineeRow = null;\n\n// 1. Exact match\ntraineeRow = allTrainees.find(row => \n  row.json['Full Name'] === traineeName\n);\n\n// 2. If no exact match, try initials matching\nif (!traineeRow) {\n  traineeRow = allTrainees.find(row => {\n    const fullName = row.json['Full Name'] || '';\n    // Extract initials from full name (e.g., \"Andrea K\" -> \"AK\")\n    const initials = fullName.split(' ').map(name => name.charAt(0)).join('');\n    return initials === traineeName;\n  });\n}\n\n// 3. If still no match, try partial matching\nif (!traineeRow) {\n  traineeRow = allTrainees.find(row => {\n    const fullName = row.json['Full Name'] || '';\n    return fullName.includes(traineeName) || traineeName.includes(fullName.split(' ')[0]);\n  });\n}\n\nif (!traineeRow) {\n  // Still not found - return error with more details\n  return [{ json: { \n    error: 'Trainee not found in master list', \n    searched_for: traineeName,\n    available_names: allTrainees.map(t => t.json['Full Name']).slice(0, 5)\n  }}];\n}\n\n// Get current completion count\nconst currentCompleted = parseInt(traineeRow.json['Total Assessments Completed'] || 0);\nconst totalRequired = parseInt(traineeRow.json['Total Assessments Required'] || 0);\n\n// Simple duplicate prevention: if current < required, increment\nconst shouldIncrement = currentCompleted < totalRequired;\n\nreturn [{\n  json: {\n    trainee_name: traineeName,\n    matched_full_name: traineeRow.json['Full Name'],\n    current_completed: currentCompleted,\n    new_completed: shouldIncrement ? currentCompleted + 1 : currentCompleted,\n    should_increment: shouldIncrement,\n    total_required: totalRequired,\n    assessment_name: assessmentName,\n    // Keep original assessment data for email updates\n    ...assessmentData\n  }\n}];"
      },
